{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Launch Chrome against localhost",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:9222/tracking.html",
            "webRoot": "${workspaceRoot}/tracking.html"
        },
        {
            "name": "Attach to Chrome",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "webRoot": "${workspaceRoot}/tracking.html"
        }
    ]
}